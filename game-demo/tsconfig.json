{
  "compilerOptions": {
      "outDir": "build",
      "baseUrl": "src",
      "sourceMap": true,
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "module": "esnext",
      "moduleResolution": "node",
      "lib": [
          "es2015",
          "dom"
      ],
      "target": "es5",
      "jsx": "react",
      "skipLibCheck": true,
      "downlevelIteration": true
  },
  "include": [
      "src"
  ]
}